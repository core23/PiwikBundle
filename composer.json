{
  "name": "core23/piwik-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides a block service for using Piwik inside the sonata-project.",
  "keywords": [
    "symfony",
    "piwik",
    "statistic",
    "sonata",
    "block",
    "widget",
    "bundle"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "php-http/httplug-bundle": "^1.1",
    "psr/log": "^1.0",
    "sonata-project/block-bundle": "^3.3",
    "sonata-project/core-bundle": "^3.0",
    "symfony/dependency-injection": "^3.4 || ^4.0",
    "symfony/expression-language": "^3.4 || ^4.0",
    "symfony/form": "^3.4 || ^4.0",
    "symfony/framework-bundle": "^3.4 || ^4.0",
    "symfony/templating": "^3.4 || ^4.0"
  },
  "require-dev": {
    "matthiasnoback/symfony-dependency-injection-test": "^1.1",
    "php-http/mock-client": "^0.3",
    "sonata-project/admin-bundle": "^3.1",
    "symfony/phpunit-bridge": "^4.0"
  },
  "suggest": {
    "php-http/buzz-adapter": "Buzz HTTP client implementation",
    "php-http/guzzle6-adapter": "Guzzle HTTP client implementation"
  },
  "autoload": {
    "psr-4": {
      "Core23\\PiwikBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\PiwikBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
