{
  "name": "core23/piwik-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides a block service for using piwik inside the sonata-project.",
  "keywords": [
    "symfony",
    "piwik",
    "statistic",
    "sonata",
    "block",
    "widget",
    "bundle"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.0",
    "php-http/httplug-bundle": "^1.1",
    "psr/log": "^1.0",
    "sonata-project/block-bundle": "^3.3",
    "sonata-project/core-bundle": "^3.0",
    "symfony/assetic-bundle": "^2.8.1",
    "symfony/framework-bundle": "^2.8.6 || ^3.1",
    "symfony/form": "^2.8.6 || ^3.1",
    "symfony/templating": "^2.8.6 || ^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "php-http/mock-client": "^0.3",
    "phpunit/phpunit": "^5.4",
    "sllh/php-cs-fixer-styleci-bridge": "^2.0",
    "sonata-project/admin-bundle": "^3.1",
    "symfony/phpunit-bridge": "^2.8 || ^3.1"
  },
  "suggest": {
    "php-http/buzz-adapter": "Buzz HTTP client implementation",
    "php-http/guzzle6-adapter": "Guzzle HTTP client implementation"
  },
  "autoload": {
    "psr-4": {
      "Core23\\PiwikBundle\\": ""
    },
    "exclude-from-classmap": [
        "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": { "Core23\\PiwikBundle\\Tests\\": "Tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
